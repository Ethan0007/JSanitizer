<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Company></Company>
    <Version>3.0.0</Version>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <Product>JoeverMonceda</Product>
    <Description>C# Json and XML string value masking/sanitizer</Description>
    <Copyright>Copyright (c) 2020 by Joever Monceda</Copyright>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <SignAssembly>false</SignAssembly>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>C#, Masking , Value Replacer , Sanitizer</PackageTags>
    <Authors>Joever M</Authors>
    <PackageProjectUrl>https://github.com/Ethan0007/JSanitizer/blob/main/README.md</PackageProjectUrl>
    <PackageIcon>js.png</PackageIcon>
    <RepositoryUrl>https://github.com/Ethan0007/JSanitizer</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageReleaseNotes>Add Configuration Path / Update configurations / Update Sensitivity / Add Positions </PackageReleaseNotes>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="sanitizerOptions.json" />
    <None Include="..\js.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Content Include="JS-README.md">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  

</Project>
